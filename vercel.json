{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/api/index.php",
      "dest": "/api/index.php"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/favicon.svg",
      "dest": "/public/favicon.svg"
    },
    {
      "src": "/script.js",
      "dest": "/public/assets/js/script.js"
    },
    {
      "src": "/style.css",
      "dest": "/public/assets/css/style.css"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
