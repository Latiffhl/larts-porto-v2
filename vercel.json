{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/api/index.php",
      "dest": "/api/index.php"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/api/pages/hero/hero.php",
      "dest": "/api/pages/hero/hero.php"
    },
    {
      "src": "/api/pages/about/about.php",
      "dest": "/api/pages/about/about.php"
    },
    {
      "src": "/api/pages/portofolio/portofolio.php",
      "dest": "/api/pages/portofolio/portofolio.php"
    },
    {
      "src": "/api/pages/skills/skills.php",
      "dest": "/api/pages/skills/skills.php"
    },
    {
      "src": "/api/pages/blogs/blogs.php",
      "dest": "/api/pages/blogs/blogs.php"
    },
    {
      "src": "/api/pages/contact/contact.php",
      "dest": "/api/pages/contact/contact.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
